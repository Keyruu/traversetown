        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "tailwindcss": "^3.3.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0"
    },
    "type": "module",
    "dependencies": {
        "@apollo/client": "^3.7.10",
        "@sveltejs/adapter-node": "^1.2.0",
        "graphql": "^16.6.0",
        "graphql-ws": "^5.12.1",
        "sass": "^1.60.0",
        "svelte-apollo": "^0.5.0",
        "ws": "^8.13.0"
    }
